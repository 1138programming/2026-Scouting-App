<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
 xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_default"
    android:clickable="true"
    tools:context=".Fragments.AutonFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <CheckBox
            android:id="@+id/noShowCheckbox2"
            android:layout_width="120sp"
            android:layout_height="120sp"
            android:layout_marginEnd="15dp"
            android:background="?android:attr/listChoiceIndicatorMultiple"
            android:backgroundTint="@color/chaminade_orange"
            android:button="@null"
            android:checked="false"
            app:layout_constraintBottom_toBottomOf="@+id/hang_attempted"
            app:layout_constraintEnd_toEndOf="@+id/testButton5"
            app:layout_constraintStart_toEndOf="@+id/teamNoShowTitle"
            app:layout_constraintTop_toTopOf="@+id/hang_attempted"
            app:layout_constraintVertical_bias="0.50" />

        <CheckBox
            android:id="@+id/noShowCheckbox3"
            android:layout_width="120sp"
            android:layout_height="120sp"
            android:background="?android:attr/listChoiceIndicatorMultiple"
            android:backgroundTint="@color/chaminade_orange"
            android:button="@null"
            android:checked="false"
            app:layout_constraintBottom_toBottomOf="@+id/hang_successful"
            app:layout_constraintEnd_toEndOf="@+id/noShowCheckbox2"
            app:layout_constraintStart_toEndOf="@+id/teamNoShowTitle"
            app:layout_constraintTop_toTopOf="@+id/hang_successful" />

        <TextView
            android:id="@+id/mainTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/auton_title"
            android:textColor="@color/off_white"
            android:textSize="96sp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/teamNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/off_white"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/redoButton"
            app:layout_constraintEnd_toStartOf="@+id/redoButton"
            app:layout_constraintStart_toEndOf="@+id/mainTitle"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/undoButton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:backgroundTint="@color/invisible"
            android:contentDescription="@string/undo_button_description"
            android:src="@drawable/undo_arrow_outline"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="@color/chaminade_orange" />

        <TextView
            android:id="@+id/undo_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/undo_title"
            android:textColor="@color/chaminade_orange"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/undoButton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/undoButton" />

        <ImageButton
            android:id="@+id/redoButton"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:backgroundTint="@color/invisible"
            android:contentDescription="@string/redo_button_description"
            android:cropToPadding="false"
            android:src="@drawable/redo_arrow_outline"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="@color/chaminade_orange" />

        <TextView
            android:id="@+id/redo_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/redo_title"
            android:textColor="@color/chaminade_orange"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/redoButton"
            app:layout_constraintTop_toBottomOf="@+id/redoButton" />

        <Button
            android:id="@+id/nextButton"
            android:layout_width="240sp"
            android:layout_height="120sp"
            android:text="@string/next_button_title"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/testButton"
            android:layout_width="240sp"
            android:layout_height="100sp"
            android:layout_marginStart="20dp"
            android:text="Test 0"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/testButton2" />

        <Button
            android:id="@+id/testButton3"
            android:layout_width="240sp"
            android:layout_height="100sp"
            android:layout_marginTop="15dp"
            android:text="Test 0"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintEnd_toEndOf="@+id/testButton2"
            app:layout_constraintTop_toBottomOf="@+id/testButton2" />

        <Button
            android:id="@+id/testButton4"
            android:layout_width="240sp"
            android:layout_height="100sp"
            android:text="Test 0"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintStart_toStartOf="@+id/testButton"
            app:layout_constraintTop_toTopOf="@+id/testButton3" />

        <Button
            android:id="@+id/testButton5"
            android:layout_width="240sp"
            android:layout_height="100sp"
            android:layout_marginTop="15dp"
            android:text="Test 0"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintEnd_toEndOf="@+id/testButton3"
            app:layout_constraintTop_toBottomOf="@+id/testButton3" />

        <Button
            android:id="@+id/testButton6"
            android:layout_width="240sp"
            android:layout_height="100sp"
            android:text="Test 0"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintStart_toStartOf="@+id/testButton4"
            app:layout_constraintTop_toTopOf="@+id/testButton5" />

        <Button
            android:id="@+id/testButton2"
            android:layout_width="240sp"
            android:layout_height="100sp"
            android:layout_marginEnd="20dp"
            android:text="Test 0"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <Button
            android:id="@+id/backButton"
            android:layout_width="240sp"
            android:layout_height="120sp"
            android:text="@string/back_button_title"
            android:textColor="@color/background_default"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="500dp"
            android:layout_height="wrap_content"
            android:src="@drawable/frcfield"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mainTitle" />

        <TextView
            android:id="@+id/fuelScored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="35dp"
            android:layout_marginTop="5dp"
            android:text="@string/fuel_scored"
            android:textColor="@color/off_white"
            android:textSize="45sp"
            app:layout_constraintStart_toStartOf="@+id/testButton6"
            app:layout_constraintTop_toBottomOf="@+id/testButton6" />

        <TextView
            android:id="@+id/hang_attempted"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="35dp"
            android:layout_marginTop="17dp"
            android:text="@string/hang_attempted"
            android:textColor="@color/off_white"
            android:textSize="45sp"
            app:layout_constraintStart_toStartOf="@+id/testButton6"
            app:layout_constraintTop_toBottomOf="@+id/fuelScored" />

        <TextView
            android:id="@+id/hang_successful"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:text="@string/hang_successful"
            android:textColor="@color/off_white"
            android:textSize="45sp"
            app:layout_constraintStart_toStartOf="@+id/hang_attempted"
            app:layout_constraintTop_toBottomOf="@+id/hang_attempted" />

        <Spinner
            android:id="@+id/fuelScoredAuton"
            android:layout_width="150sp"
            android:layout_height="50dp"
            android:layout_marginEnd="20dp"
            android:textColor="@color/off_white"
            android:textSize="64sp"
            app:layout_constraintBottom_toBottomOf="@+id/fuelScored"
            app:layout_constraintEnd_toEndOf="@+id/testButton5"
            app:layout_constraintTop_toTopOf="@+id/fuelScored" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
